{
  "dependencies": {
    "axios": "^1.7.2",
    "clickup.js": "^4.0.1",
    "cron-parser": "^4.9.0",
    "dotenv": "^17.2.3",
    "highcharts-more": "^0.1.7",
    "is-json": "^2.0.1",
    "jimp": "^1.6.0",
    "js-yaml": "^4.1.0",
    "loggly": "^1.1.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mysql2": "^3.12.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.0",
    "openai-gpt-token-counter": "^1.1.1",
    "openmeteo": "^1.1.5",
    "react-flip-move": "^3.0.5",
    "string-similarity": "^4.0.4",
    "winston": "^3.18.3",
    "winston-loggly-bulk": "^3.3.3",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@playwright/test": "^1.57.0",
    "concurrently": "^7.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.4",
    "ws": "^8.19.0"
  },
  "scripts": {
    "dev": "npm run start:dev",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start:dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend": "node backend/index.js",
    "backend:dev": "nodemon --watch backend --ext js,mjs,json,yml backend/index.js",
    "backend:log": "node backend/index.js 2>&1 | tee dev.log",
    "frontend": "npm run dev --prefix frontend",
    "frontend:dev": "npm run dev --prefix frontend",
    "test": "npm run test:unit && npm run test:assembly",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --testPathPattern=tests/unit",
    "test:assembly": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --testPathPattern=tests/assembly",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --testPathPattern=tests/integration",
    "test:smoke": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --testPathPattern=tests/smoke",
    "test:live": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --testPathPattern=tests/live",
    "test:pre-commit": "npm run test:unit && npm run test:assembly && npm run test:integration",
    "test:pre-deploy": "npm run test:pre-commit && npm run test:smoke && npm run test:live",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --testPathPattern=tests/unit",
    "test:all": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "rt": "npx playwright test tests/runtime --headed",
    "rt:player": "npx playwright test tests/runtime/player --headed",
    "rt:fitness": "npx playwright test tests/runtime/fitness-session --headed",
    "rt:finance": "npx playwright test tests/runtime/finance --headed",
    "rt:payroll": "npx playwright test tests/runtime/finance --grep 'trigger sync' --headed --reporter=list",
    "audit:logging": "node scripts/audit-logging-code.mjs",
    "audit:logging:frontend": "node scripts/audit-logging-code.mjs --baseDir=frontend/src",
    "audit:logging:backend": "node scripts/audit-logging-code.mjs --baseDir=backend"
  }
}
