{
  "dependencies": {
    "axios": "^1.7.2",
    "cron-parser": "^4.9.0",
    "highcharts-more": "^0.1.7",
    "is-json": "^2.0.1",
    "jimp": "^1.6.0",
    "js-yaml": "^4.1.0",
    "loggly": "^1.1.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mysql2": "^3.12.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.0",
    "openai-gpt-token-counter": "^1.1.1",
    "openmeteo": "^1.1.5",
    "react-flip-move": "^3.0.5",
    "string-similarity": "^4.0.4",
    "winston": "^3.18.3",
    "winston-loggly-bulk": "^3.3.3",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "concurrently": "^7.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.4"
  },
  "scripts": {
    "dev": "npm run start:dev",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start:dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend": "node backend/index.js",
    "backend:dev": "nodemon --watch backend --ext js,mjs,json,yml backend/index.js 2>&1 | tee -a dev.log",
    "backend:log": "node backend/index.js 2>&1 | tee dev.log",
    "frontend": "npm run dev --prefix frontend",
    "frontend:dev": "npm run dev --prefix frontend 2>&1 | tee -a dev.log",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:harvest": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand backend/harvest.test.mjs",
    "test:harvest:integration": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand backend/harvest.integration.test.mjs",
    "test:all:harvest": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand backend/harvest*.test.mjs",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:nutribot": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand backend/chatbots/nutribot/_test"
  }
}
