{
  "dependencies": {
    "axios": "^1.7.2",
    "canvas": "^3.2.1",
    "clickup.js": "^4.0.1",
    "cron-parser": "^4.9.0",
    "dotenv": "^17.2.3",
    "highcharts-more": "^0.1.7",
    "html-entities": "^2.6.0",
    "is-json": "^2.0.1",
    "jimp": "^1.6.0",
    "js-yaml": "^4.1.0",
    "loggly": "^1.1.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "music-metadata": "^11.10.6",
    "mysql2": "^3.12.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.0",
    "openai-gpt-token-counter": "^1.1.1",
    "openmeteo": "^1.1.5",
    "react-flip-move": "^3.0.5",
    "smartquotes": "^2.3.2",
    "string-similarity": "^4.0.4",
    "winston": "^3.18.3",
    "winston-loggly-bulk": "^3.3.3",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "imports": {
    "#backend/*": "./backend/*",
    "#frontend/*": "./frontend/src/*",
    "#fixtures/*": "./tests/_fixtures/*",
    "#testlib/*": "./tests/lib/*",
    "#extensions/*": "./_extensions/*"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@playwright/test": "^1.57.0",
    "concurrently": "^7.6.0",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.4",
    "ws": "^8.19.0"
  },
  "scripts": {
    "dev": "npm run start:dev",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start:dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend": "node backend/src/server.mjs",
    "backend:dev": "nodemon --watch backend --ext js,mjs,json,yml backend/src/server.mjs",
    "backend:log": "node backend/src/server.mjs 2>&1 | tee dev.log",
    "frontend": "npm run dev --prefix frontend",
    "frontend:dev": "npm run dev --prefix frontend",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node tests/unit/harness.mjs",
    "test:integration": "node tests/integration/harness.mjs",
    "rt": "npx playwright test tests/runtime --headed",
    "rt:player": "npx playwright test tests/runtime/player --headed",
    "rt:fitness": "npx playwright test tests/runtime/fitness-session --headed",
    "rt:finance": "npx playwright test tests/runtime/finance --headed"
  }
}
